								<descr>Time buffer (uS)</descr>
							</var>
							<var name="Module" type="A1212_Module"/>
							<var name="Temperature" type="REAL"/>
							<var name="AOut" type="REAL" dim0="2">
								<descr>Analog out setting (4-20 mA)</descr>
							</var>
