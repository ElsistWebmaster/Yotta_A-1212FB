					<sourceCode type="ST">
						<![CDATA[/*  ****************************************************************************
 FUNCTION BLOCK "A1212_Module"
********************************************************************************
 Questo programma  parte del progetto Yotta_A-1212FB.
 Link: https://github.com/ElsistWebmaster/Yotta_A-1212FB

 Dichiazaione variabili:

VAR
	TCPClient : SysTCPClient;	(* TCPClient management *)
	MdbCfg : ARRAY[ 0..2 ] OF MODBUSCONFIG;	(* Modbus configuration *)
	MMdb : ModbusMaster;	(* Modbus master communication *)
	MIDx : USINT;	(* Modbus index *)
	TimeBf : UDINT;	(* Time buffer (uS) *)
END_VAR


VAR_OUTPUT
	Enabled : BOOL;	(* FB enabled *)
	Done : BOOL; (* FB done *)
	CError : BOOL;	(* Communication error *)
	CECounter : UDINT;	(* Communication error counter *)
	Di : ARRAY[ 0..1 ] OF BOOL;	(* Digital inputs *)
	Ai : ARRAY[0..3] OF REAL; (* Analog inputs *)
END_VAR

VAR_INPUT
	Enable : BOOL;	(* FB enable *)
	Address : @USINT;	(* Module address *)
	Port : UINT;	(* Module port *)
	Do : ARRAY[ 0..1 ] OF BOOL;	(* Digital ouputs *)
	Ao : ARRAY[ 0..1 ] OF UINT;	(* Analog outputs *)
END_VAR

***************************************************************************** */

	(* ---------------------------------------------------------------------- *)
	(* VARIABILI ONE SHOT                                                     *)
	(* ---------------------------------------------------------------------- *)
	(* Eseguo reset variabili one shot. *)

	Done:=FALSE; (* FB done *)

	(* ---------------------------------------------------------------------- *)
	(* INIZIALIZZAZIONE                                                       *)
	(* ---------------------------------------------------------------------- *)
	(* Se primo loop di abilitazione inizializzo variabili. *)

	IF ((Enable) AND NOT(Enabled)) THEN
		Enabled:=TRUE; (* FB enabled *)

		(* TCPClient initialization. *)

		TCPClient.PeerAdd:=Address; (* Peer address *)
		TCPClient.PeerPort:=Port; (* Peer port *)
		TCPClient.LocalAdd:=ADR('0.0.0.0'); (* Local address *)
		TCPClient.LocalPort:=0; (* Local port *)
		TCPClient.FlushTm:=50; (* Flush time (mS) *)
		TCPClient.LifeTm:=20; (* Life time (S) *)
		TCPClient.RxSize:=128; (* Rx buffer size *)
		TCPClient.TxSize:=128; (* Tx buffer size *)

		(* Modbus master initialization. *)

		MMdb.SpyOn:=TRUE; (* Spionaggio attivo *)
		MMdb.Node:=1; (* Modbus node *)
		MMdb.Type:=2; (* Modbus type *)
		MMdb.IFTime:=0; (* Tempo tra frames *)
		MMdb.Timeout:=500; (* Communication timeout *)
		MMdb.Delay:=500; (* Communication delay *)

		(* Inizializzazioni generali. *)

		MIDx:=0; (* Modbus index *)
		TimeBf:=SysGetSysTime(TRUE); (* Communication error *)
	END_IF;

	(* ---------------------------------------------------------------------- *)
	(* CONNESSIONE A MODULO                                                   *)
	(* ---------------------------------------------------------------------- *)
	(* Gestione connessione al modulo. *)

	TCPClient(Connect:=Enabled AND NOT(CError)); (* TCPClient management *)

	(* ---------------------------------------------------------------------- *)
	(* GESTIONE COMUNICAZIONE MODBUS                                          *)
	(* ---------------------------------------------------------------------- *)
	(* Gestione comunicazione modbus. *)

	MMdb.File:=TCPClient.File; (* File pointer *)
	IF (MMdb.Done) THEN MMdb.Enable:=FALSE; END_IF;
	IF NOT(TCPClient.Connected) THEN MMdb.Enable:=FALSE; END_IF;
	MMdb(); (* Modbus master communication *)

	(* Se FB non abilitato esco. *)

	IF NOT(Enable) THEN Enabled:=FALSE; Done:=FALSE; RETURN; END_IF;

	(* ---------------------------------------------------------------------- *)
	(* CONTROLLO ERRORE COMUNICAZIONE MODBUS                                  *)
	(* ---------------------------------------------------------------------- *)
	(* Controllo se Ok comunicazione *)

	IF NOT(CError) THEN
		IF (MMdb.Ok) THEN TimeBf:=SysGetSysTime(TRUE); END_IF;

		(* Se timeout errore comunicazione. *)
		
		IF ((SysGetSysTime(TRUE)-TimeBf) > (60*1000000)) THEN
			TimeBf:=SysGetSysTime(TRUE); (* Communication error *)
			CECounter:=CECounter+1; (* Communication error counter *)
			CError:=TRUE; (* Communication error *)
		END_IF;
	END_IF;

	(* Su errore, blocco connessione al modulo per tempo superiore al tempo *)
	(* di reset della connessione attiva sul modulo (5 minuti). *)

	IF (CError) THEN
		IF ((SysGetSysTime(TRUE)-TimeBf) > (330*1000000)) THEN
			TimeBf:=SysGetSysTime(TRUE); (* Communication error *)
			CError:=FALSE; (* Communication error *)
		END_IF;
	END_IF;

	(* ---------------------------------------------------------------------- *)
	(* PARAMETRIZZAZIONE COMUNICAZIONE MODBUS                                 *)
	(* ---------------------------------------------------------------------- *)
	(* Eseguo parametrizzazione FB modbus. *)

	IF ((TCPClient.Connected) AND NOT(MMdb.Enable)) THEN
		MMdb.Enable:=TRUE; (* FB enable *)

		(* Eseguo incremento indice configurazione Modbus. *)

		MIDx:=MIDx+1; (* Modbus index *)
		IF (MIDx > 3) THEN MIDx:=0;	Done:=TRUE; END_IF;

		(* Configurazione modbus. *)

		CASE (MIDx) OF

			(* -------------------------------------------------------------- *)
			(* 0~1 DI Input Signal. *)

			0:
			MMdb.FCode:=16#01; (* Modbus function *)
			MMdb.Address:=1; (* Variable address *)
			MMdb.Buffer:=ADR(Di); (* Buffer address *)
			MMdb.Points:=SIZEOF(Di); (* Number of points *)

			(* -------------------------------------------------------------- *)
			(* 0~1 DO Output Value *)

			1:
			MMdb.FCode:=16#0F; (* Modbus function *)
			MMdb.Address:=17; (* Variable address *)
			MMdb.Buffer:=ADR(Do); (* Buffer address *)
			MMdb.Points:=SIZEOF(Do); (* Number of points *)

			(* -------------------------------------------------------------- *)
			(* 0~1 Current Input Value *)
	
			2:
			MMdb.FCode:=16#03; (* Modbus function *)
			MMdb.Address:=641; (* Variable address *)
			MMdb.Buffer:=ADR(Ai); (* Buffer address *)
			MMdb.Points:=SIZEOF(Ai)/2; (* Number of points *)

			(* -------------------------------------------------------------- *)
			(* 0~1 Current Output Value (0~4000:0/4~20mA). *)

			3:
			MMdb.FCode:=16#10; (* Modbus function *)
			MMdb.Address:=17; (* Variable address *)
			MMdb.Buffer:=ADR(Ao); (* Buffer address *)
			MMdb.Points:=SIZEOF(Ao)/2; (* Number of points *)
		END_CASE;
	END_IF;

(* [End of file] *)
]]>
