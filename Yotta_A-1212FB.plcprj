						<![CDATA[(* ************************************************************************** *)
(* PROGRAM "Logic"                                                            *)
(* ************************************************************************** *)
(* Gestione logica.                                                           *)
(* -------------------------------------------------------------------------- *)

	(* ---------------------------------------------------------------------- *)
	(* GESTIONE MODULO YOTTA                                                  *)
	(* ---------------------------------------------------------------------- *)
	(* Eseguo gestione modulo Yotta. *)

	Module.Address:=ADR('192.168.0.184'); (* Module address *)
	Module.Port:=502; (* Module port *)
	Module(Enable:=TRUE); (* Module management *)
	IF NOT(Module.Done) THEN RETURN; END_IF;

	(* ---------------------------------------------------------------------- *)
	(* GESTIONE INGRESSI LOGICI                                               *)
	(* ---------------------------------------------------------------------- *)
	(* Copio stato Di su uscite modulo CPU. *)

	Do00CPU:=Module.Di[0]; (* Do00 CPU module *)
	Do01CPU:=Module.Di[1]; (* Do01 CPU module *)

	(* ---------------------------------------------------------------------- *)
	(* GESTIONE USCITE LOGICHE                                                *)
	(* ---------------------------------------------------------------------- *)
	(* Copio stato Di00CPU su uscita Do0. *)

	Module.Do[0]:=Di00CPU;

	(* Eseguo gestione lampeggio uscita Do1. *)

	IF ((SysGetSysTime(TRUE)-TimeBf) > 5000000) THEN
		TimeBf:=SysGetSysTime(TRUE); (* Time buffer (uS) *)
		Module.Do[1]:=NOT(Module.Do[1]);
	END_IF;

	(* ---------------------------------------------------------------------- *)
	(* GESTIONE INGRESSI ANALOGICI                                            *)
	(* ---------------------------------------------------------------------- *)
	(* Copio stato ingressi analogici. *)

	Temperature:=Module.Ai[2];

	(* ---------------------------------------------------------------------- *)
	(* GESTIONE USCITE ANALOGICHE                                             *)
	(* ---------------------------------------------------------------------- *)
	(* Imposto uscite analogiche. Il modulo  settato in modalit 4-20 mA. *)

	Module.Ao[0]:=TO_UINT(((AOut[0]-4.0)*(4000.0/(20.0-4.0)))+4.0);
	Module.Ao[1]:=TO_UINT(((AOut[1]-4.0)*(4000.0/(20.0-4.0)))+4.0);

(* [End of file] *)
]]>
